{% extends 'base.html.twig' %}

{% block title %}Compétences{% endblock %}

{% block body %}
    <div class="container">
    <h1>Compétences</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ competence.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ competence.nom }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-primary"  href="{{ path('competence_index') }}">Retour</a>

    <a class="btn btn-warning" href="{{ path('competence_edit', {'id': competence.id}) }}">Modifier</a>
    {% if  %}
    {{ include('competence/_delete_form.html.twig') }}
    <div/>
{% endblock %}
